    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tailwindcss-rtl": "^0.9.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
