    "react-dom": "^19.2.0",
    "react-i18next": "^16.4.1",
    "react-router-dom": "^7.10.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
